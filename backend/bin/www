#!/usr/bin/env node

const app = require('../app');
const http = require('http');

const port = process.env.PORT || '9000';

app.set('port', port);

const server = http.createServer(app);

server.listen(port);
server.on('listening', () => {
    console.log(`Express server running on http://127.0.0.1:${port}`);
});
